5 REM HIT LEFT PLAYER
7 DIM FILE$(100)
8 FILE$="H6:JUMP.ASM"
9 OPEN #5,8,0,FILE$
10 GRAPHICS 15
15 GOSUB 5000
40 COLOR 1
50 PLOT SX,SY
70 SX=SX+ACCX
80 SY=SY+ACCY
90 GOSUB 10000
95 ACCY=ACCY+GRAV
100 GOTO 70
1999 REM SPEED 1
2000 SX=35:SY=110:GRAV=0.21
2005 ACCX=1:ACCY=-1.5
2010 RETURN 
2999 REM SPEED 2
3000 SX=35:SY=110:GRAV=0.11
3005 ACCX=1.2:ACCY=-1.5
3010 RETURN 
3999 REM SPEED 3
4000 SX=35:SY=110:GRAV=0.11
4005 ACCX=2.22:ACCY=-1.5
4010 RETURN 
4999 REM SPEED 4
5000 SX=35:SY=110:GRAV=0.11
5005 ACCX=5.22:ACCY=-1.5
5010 RETURN 
10000 PLOT SX,SY
10010 PUT #5,ASC("D")
10011 PUT #5,ASC("T")
10012 PUT #5,ASC("A")
10013 PUT #5,ASC(" ")
10014 PUT #5,ASC("(")
10015 GOSUB 20000
10016 PUT #5,ASC(")")
10017 PUT #5,10
10020 RETURN 
20000 OSX=SX:SETKI=-1:IF SX>100 THEN SETKI=INT(SX/100)
20010 IF SETKI<>-1 THEN FOR I=1 TO SETKI:SX=SX-100:NEXT I
20015 DYSZKI=-1:IF SX>10 THEN DYSZKI=INT(SX/10)
20020 IF DYSZKI<>-1 THEN FOR I=1 TO DYSZKI:SX=SX-10:NEXT I
20500 IF SETKI<>-1 THEN PUT #5,SETKI+48
20501 IF SETKI=-1 THEN PUT #5,48
20510 IF DYSZKI<>-1 THEN PUT #5,DYSZKI+48
20511 IF DYSZKI=-1 THEN PUT #5,0+48
20520 PUT #5,INT(SX)+48
20999 SX=OSX:RETURN 

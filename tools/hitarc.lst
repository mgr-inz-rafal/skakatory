5 REM HIT LEFT PLAYER
7 DIM FILE$(100),HEAD$(100),YY(200):KTY=0
8 FILE$="H6:JUMP.ASM"
9 OPEN #5,8,0,FILE$
10 GRAPHICS 15
15 GOSUB 2000
40 COLOR 1
50 PLOT SX,SY-(156-110)
60 HEAD$="LEFT_KILL_X_SPEED_1":GOSUB 21000
65 YY(KTY)=SY:KTY=KTY+1
70 SX=SX+ACCX
80 SY=SY+ACCY
90 GOSUB 10000
95 ACCY=ACCY+GRAV
100 GOTO 65
500 HEAD$=" ":GOSUB 21000:HEAD$="LEFT_KILL_Y_SPEED_1":GOSUB 21000
510 FOR Y=0 TO KTY-1:DI=INT(YY(Y)):GOSUB 25000:NEXT Y
520 END 
1999 REM SPEED 1
2000 SX=80:SY=156:GRAV=0.21
2005 ACCX=1:ACCY=-1.5
2010 RETURN 
2999 REM SPEED 2
3000 SX=35:SY=110:GRAV=0.11
3005 ACCX=1.2:ACCY=-1.5
3010 RETURN 
3999 REM SPEED 3
4000 SX=35:SY=110:GRAV=0.11
4005 ACCX=2.22:ACCY=-1.5
4010 RETURN 
4999 REM SPEED 4
5000 SX=35:SY=110:GRAV=0.11
5005 ACCX=5.22:ACCY=-1.5
5010 RETURN 
10000 DI=INT(SX):GOSUB 25000
10010 TRAP 500:PLOT SX,SY-(156-110)
10020 RETURN 
20000 REM === PAINT DIGIT FROM "DI" ===
20001 ODI=DI:SETKI=-1:IF DI>=100 THEN SETKI=INT(DI/100)
20010 IF SETKI<>-1 THEN FOR I=1 TO SETKI:DI=DI-100:NEXT I
20015 DYSZKI=-1:IF DI>=10 THEN DYSZKI=INT(DI/10)
20020 IF DYSZKI<>-1 THEN FOR I=1 TO DYSZKI:DI=DI-10:NEXT I
20500 IF SETKI<>-1 THEN PUT #5,SETKI+48
20501 IF SETKI=-1 THEN PUT #5,48
20510 IF DYSZKI<>-1 THEN PUT #5,DYSZKI+48
20511 IF DYSZKI=-1 THEN PUT #5,0+48
20520 PUT #5,INT(DI)+48
20999 DI=ODI:RETURN 
21000 REM === WRITE DTA HEADER ===
21010 FOR Q=1 TO LEN(HEAD$):PUT #5,ASC(HEAD$(Q,Q)):NEXT Q
21015 PUT #5,10
21020 RETURN 
25000 REM === PUT DTA('DI') LINE ===
25009 FOR Q=1 TO 12:PUT #5,ASC(" "):NEXT Q
25010 PUT #5,ASC("d")
25011 PUT #5,ASC("t")
25012 PUT #5,ASC("a")
25013 PUT #5,ASC(" "):PUT #5,ASC("b")
25014 PUT #5,ASC("(")
25015 GOSUB 20000
25016 PUT #5,ASC(")")
25017 PUT #5,10
25020 RETURN 
